{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "es6": true,
        "ecmaVersion": 12,
        "sourceType": "module"
    },

    "parser": "babel-eslint",

    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": 0
    }
}
